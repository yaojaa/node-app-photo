{{#each list}}
<div class="media">
    <div class="media-left">
        <a href="/space/{{userId}}"><img width="20" alt="" src="{{userAvatar}}?imageView2/1/w/50/"></a>
    </div>
    <div class="media-body">
        <p class="info"><strong><a href="/space/{{userId}}">{{userName}}</a>:</strong>{{content}}</p>
        <p class="date">发布时间：{{createAt}}</p>
    </div>
</div>
{{/each}}
